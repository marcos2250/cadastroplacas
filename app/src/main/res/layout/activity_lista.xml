<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="360px">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:minHeight="360px"
        android:orientation="vertical">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:id="@+id/btnOrdSerial"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:onClick="serial"
                android:text="SER" />

            <Button
                android:id="@+id/btnOrdPlaca"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:onClick="placa"
                android:text="PLA" />

            <Button
                android:id="@+id/btnIncompletos"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:onClick="incompletos"
                android:text="ERR" />

            <Button
                android:id="@+id/btnLoc"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:onClick="localizar"
                android:text="LOC" />

            <Button
                android:id="@+id/btnNovo"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:onClick="novo"
                android:text="CAD" />

            <Button
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="I/E"
                android:onClick="importarExportar"/>

        </GridLayout>


        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:minHeight="360px"
            android:id="@+id/lista"/>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
